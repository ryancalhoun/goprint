{
   "manifest_version": 2,
   "name": "Go Print",
   "permissions": [ "pageCapture", "tabs", "contextMenus" ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "background": {
      "scripts": [ "print.js" ]
   },
   "icons": {
      "128": "print-128.png",
      "32": "print-32.png"
   }
}
